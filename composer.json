{
    "name": "aoliverio/builder",
    "description": "Bake template for CakePHP 3.x",
    "type": "cakephp-plugin",
    "license":"MIT",
    "authors":[
        {
            "name":"Antonio Oliverio",
            "role":"Author",
            "homepage":"http://www.aoliverio.com/"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Builder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Builder\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
